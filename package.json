{
  "name": "vimflowy",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": "0.10.x",
    "npm": "1.2.x"
  },
  "scripts": {
    "start": "node_modules/.bin/gulp",
    "test": "node_modules/.bin/mocha --compilers js:babel-core/register test/tests",
    "coverage": "multi='spec=- html-cov=public/coverage.html' node_modules/.bin/mocha  --compilers js:babel-core/register test/tests --require blanket --reporter mocha-multi test/tests/*",
    "build": "node_modules/.bin/gulp assets:prod"
  },
  "config": {
    "blanket": {
      "pattern": "//assets/js/.*\\.js$/"
    }
  },
  "dependencies": {
    "async": "latest",
    "bluebird": "latest",
    "express": "latest",
    "lodash": "latest",
    "tv4": "latest"
  },
  "devDependencies": {
    "babel-core": "latest",
    "babel-preset-es2015": "latest",
    "babelify": "latest",
    "blanket": "latest",
    "browserify": "latest",
    "del": "latest",
    "gulp": "latest",
    "gulp-babel": "latest",
    "gulp-jade": "latest",
    "gulp-mocha": "latest",
    "gulp-rename": "latest",
    "gulp-sass": "latest",
    "gulp-sourcemaps": "latest",
    "gulp-streamify": "latest",
    "gulp-uglify": "latest",
    "gulp-util": "latest",
    "mocha": "latest",
    "mocha-multi": "latest",
    "require-globify": "latest",
    "stream-to-array": "latest",
    "vinyl-buffer": "latest",
    "vinyl-source-stream": "latest"
  }
}
